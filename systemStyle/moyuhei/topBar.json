{
  "QWidget": {
    "background-color": "#004D40"
  },
  "QPushButton": {
    "border": "none",
    "border-radius": "5px",
    "padding": "5px",
    "background-color": "#004D40",
    "color": "#E0E0E0"
  },
  "QPushButton:hover": {
    "background-color": "#00695C"
  },
  "QPushButton:pressed": {
    "background": "#00897B"
  },
  "QPushButton::menu-indicator": {
    "image": "none",
    "width": "0px"
  }
}
